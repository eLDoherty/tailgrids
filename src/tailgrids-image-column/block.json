{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/tailgrids-image-column",
	"version": "0.1.0",
	"title": "Tailgrids Image Column",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"attributes": {
		"heading": {
			"type": "string"
		},
		"caption": {
			"type": "string"
		},
		"imageId": {
			"type": "number"
		},
		"imageUrl": {
			"type": "string"
		},
		"position": {
			"type": "boolean",
			"default": true
		}
	},
	"textdomain": "tailgrids-image-column",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}